te path=\"/dashboard/myTickets/:id\" component={MyTickets} />\n          <Route path=\"*\" component={() => '404 NOT FOUND'} />\n        </Switch>\n      </BrowserRouter>\n    </ThemeProvider>\n  );\n}\n",{"ruleId":"102","severity":1,"message":"103","line":87,"column":7,"nodeType":"104","messageId":"105","endLine":87,"endColumn":20},{"ruleId":"102","severity":1,"message":"103","line":33,"column":7,"nodeType":"104","messageId":"105","endLine":33,"endColumn":20},{"ruleId":"102","severity":1,"message":"103","line":49,"column":7,"nodeType":"104","messageId":"105","endLine":49,"endColumn":20},{"ruleId":"102","severity":1,"message":"103","line":63,"column":7,"nodeType":"104","messageId":"105","endLine":63,"endColumn":18},{"ruleId":"102","severity":1,"message":"103","line":65,"column":7,"nodeType":"104","messageId":"105","endLine":65,"endColumn":20},{"ruleId":"102","severity":1,"message":"103","line":80,"column":7,"nodeType":"104","messageId":"105","endLine":80,"endColumn":18},{"ruleId":"102","severity":1,"message":"103","line":82,"column":7,"nodeType":"104","messageId":"105","endLine":82,"endColumn":20},{"ruleId":"102","severity":1,"message":"103","line":97,"column":7,"nodeType":"104","messageId":"105","endLine":97,"endColumn":18},{"ruleId":"102","severity":1,"message":"103","line":99,"column":7,"nodeType":"104","messageId":"105","endLine":99,"endColumn":20},{"ruleId":"102","severity":1,"message":"103","line":129,"column":7,"nodeType":"104","messageId":"105","endLine":129,"endColumn":20},{"ruleId":"102","severity":1,"message":"103","line":149,"column":5,"nodeType":"104","messageId":"105","endLine":149,"endColumn":16},{"ruleId":"102","severity":1,"message":"103","line":150,"column":5,"nodeType":"104","messageId":"105","endLine":150,"endColumn":16},{"ruleId":"102","severity":1,"message":"103","line":151,"column":5,"nodeType":"104","messageId":"105","endLine":151,"endColumn":16},{"ruleId":"102","severity":1,"message":"103","line":155,"column":5,"nodeType":"104","messageId":"